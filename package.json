{
  "name": "chipka",
  "version": "0.1.0",
  "main": "./main.js",
  "scripts": {
    "babel-watch": "./node_modules/.bin/babel --watch --presets react  app/js/view --out-file app/js/lib/_index.js",
    "build": "./node_modules/.bin/babel --presets react app/js/view  --out-file app/js/lib/_index.js",
    "start": "electron ./app",
    "package": "cp -r build app/  && electron-packager ./app chipka --platform darwin --arch x64 --out ~/Desktop/chipka --version 0.30.2 --overwrite --icon=./app/img/app-icon.icns",
    "test": "jasmine-node --verbose --captureExceptions test",
    "testUI": "jest",
    "dist":"node_modules/.bin/build --platform darwin --arch x64"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-jest": "^9.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "electron-builder": "^3.14.0",
    "electron-packager": "^5.0.1",
    "electron-prebuilt": "^0.30.3",
    "jasmine-core": "^2.4.1",
    "jest-cli": "*",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "mock-require": "^1.2.1",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "~0.14.0"
  },
  "build": {
    "app-bundle-id": "chipka",
    "app-category-type": "utility",
    "osx": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils"
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "lodash": "^4.11.1",
    "nconf": "^0.7.2",
    "nedb": "^1.7.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  }
}
